{
  "restartable": "rs",
  "colours": true,
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "dist",
    "test"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": ""
  },
  "delay": 10,
  "exec": "rm -rf dist && babel server -d dist && node index.js",
  "ext": "js json"
}